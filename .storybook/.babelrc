{
  "presets": ["@babel/preset-react", ["@babel/preset-env"]],
  "plugins": [
    "emotion",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-optional-chaining",
    [
      "module-resolver",
      {
        "alias": {
          "src": "./src"
        }
      }
    ],
    ["inline-react-svg", { "ignorePattern": "(?<!\\.component)\\.svg$" }]
  ]
}
