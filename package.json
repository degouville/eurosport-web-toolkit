{
  "name": "@eurosport/web-toolkit",
  "version": "1.8.2",
  "description": "Eurosport Web Toolkit",
  "main": "dist/cjs/index.js",
  "browser": "dist/esm/index.js",
  "sideEffects": false,
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "jest -w 1 --watch",
    "test:coverage": "jest -w 1 --coverage --watchAll=false",
    "test:ci": "jest -w 1",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "lint": "eslint .",
    "prettier": "prettier --write '**/{*.{js?(on),ts,y?(a)ml,graphql,md,scss},.*.y?(a)ml,.*.json,.prettierrc,.stylelintrc}'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook --quiet",
    "start-static-storybook": "serve ./storybook-static",
    "clean": "rm -rf dist",
    "build:babel": "babel src --out-dir dist/cjs --ignore 'src/**/*.stories.js' --ignore 'src/**/*.test.js'",
    "build:babel:esm": "MODULE=true babel src --out-dir dist/esm --ignore 'src/**/*.stories.js' --ignore 'src/**/*.test.js'",
    "build": "npm run clean && npm run build:babel && npm run build:babel:esm",
    "prepublish": "npm run build",
    "release": "release-it"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EurosportDigital/web-toolkit.git"
  },
  "author": "Eurosport Web Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EurosportDigital/web-toolkit/issues"
  },
  "homepage": "https://github.com/EurosportDigital/web-toolkit#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-backgrounds": "^5.1.9",
    "@storybook/addon-info": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addon-options": "^5.1.9",
    "@storybook/addon-viewport": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@testing-library/react-hooks": "^1.1.0",
    "axios": "^0.19.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^9.2.11",
    "babel-plugin-file-loader": "^1.1.1",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "cypress": "^3.4.1",
    "emotion": "^9.2.12",
    "emotion-theming": "^9.2.9",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "helmet": "^3.20.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-emotion": "^9.2.11",
    "lint-staged": "^8.2.1",
    "polished": "^2.3.3",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-emotion": "^9.2.12",
    "react-helmet": "^5.2.1",
    "release-it": "^12.3.5",
    "serve": "^11.1.0",
    "wait-on": "^3.3.0"
  },
  "peerDependencies": {
    "emotion": "9",
    "emotion-theming": "9",
    "helmet": "3",
    "polished": "2",
    "prop-types": "15",
    "react": "16",
    "react-emotion": "9",
    "react-helmet": "5"
  },
  "dependencies": {
    "@eurosport/toolkit-player": "^0.5.25",
    "core-js": "^3.1.4",
    "hoist-non-react-statics": "^3.3.0",
    "lodash": "^4.17.14",
    "rc-slider": "^8.6.13",
    "react-device-detect": "^1.7.5",
    "react-google-recaptcha": "^2.0.0-rc.1",
    "react-lines-ellipsis": "^0.14.1",
    "react-sizeme": "^2.6.7",
    "screenfull": "^4.2.1"
  }
}
