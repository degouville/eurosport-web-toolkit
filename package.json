{
  "name": "@eurosport/web-toolkit",
  "version": "0.31.2",
  "description": "Eurosport Web Toolkit",
  "main": "dist/cjs/index.js",
  "browser": "dist/esm/index.js",
  "sideEffects": false,
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "npm run test:ci -- --watch",
    "test:ci": "jest -w 1",
    "test:visual": "STORYBOOK_OVERRIDE_BG=true eyes-storybook",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "lint": "eslint .",
    "prettier": "prettier --write '**/{*.{js?(on),ts,y?(a)ml,graphql,md,scss},.*.y?(a)ml,.*.json,.prettierrc,.stylelintrc}'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "start-static-storybook": "serve ./storybook-static",
    "clean": "rm -rf dist",
    "build:babel": "babel src --out-dir dist/cjs --ignore 'src/**/*.stories.js' --ignore 'src/**/*.test.js'",
    "build:babel:esm": "MODULE=true babel src --out-dir dist/esm --ignore 'src/**/*.stories.js' --ignore 'src/**/*.test.js'",
    "build": "npm run clean && npm run build:babel && npm run build:babel:esm",
    "prepublish": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EurosportDigital/web-toolkit.git"
  },
  "author": "Eurosport Web Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EurosportDigital/web-toolkit/issues"
  },
  "homepage": "https://github.com/EurosportDigital/web-toolkit#readme",
  "devDependencies": {
    "@applitools/eyes-storybook": "^2.6.25",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@storybook/addon-actions": "^4.1.18",
    "@storybook/addon-backgrounds": "^4.1.18",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addon-knobs": "^4.1.18",
    "@storybook/addon-links": "^4.1.18",
    "@storybook/addon-options": "^4.1.18",
    "@storybook/addon-viewport": "^4.1.18",
    "@storybook/addons": "^4.1.18",
    "@storybook/react": "^4.1.18",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^9.2.11",
    "babel-plugin-file-loader": "^1.1.1",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "cypress": "^3.3.1",
    "emotion": "^9.2.12",
    "emotion-theming": "^9.2.9",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "jest-emotion": "^9.2.11",
    "lint-staged": "^8.1.7",
    "polished": "^2.3.3",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-emotion": "^9.2.12",
    "serve": "^11.0.1",
    "wait-on": "^3.2.0"
  },
  "peerDependencies": {
    "emotion": "9",
    "emotion-theming": "9",
    "polished": "2",
    "prop-types": "15",
    "react": "16",
    "react-emotion": "9"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.3.0",
    "lodash": "^4.17.11",
    "react-lines-ellipsis": "^0.14.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/testSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
